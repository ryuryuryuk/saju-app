{
  "name": "saju-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20 <24"
  },
  "scripts": {
    "predev": "node scripts/check-node-version.mjs && node -e \"require('fs').rmSync('.next/dev', { recursive: true, force: true })\"",
    "dev": "NEXT_TELEMETRY_DISABLED=1 next dev --webpack --hostname 127.0.0.1 --port 3000",
    "dev:turbo": "NEXT_TELEMETRY_DISABLED=1 next dev --turbopack --hostname 127.0.0.1 --port 3000",
    "dev:reset": "rm -rf .next; NEXT_TELEMETRY_DISABLED=1 next dev --webpack --hostname 127.0.0.1 --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@supabase/supabase-js": "^2.95.3",
    "dotenv": "^17.3.1",
    "next": "16.1.6",
    "openai": "^6.21.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "pg": "^8.18.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
